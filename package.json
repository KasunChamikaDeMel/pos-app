{
  "name": "pos-system",
  "version": "1.0.0",
  "description": "Comprehensive Point of Sale Desktop Application",
  "main": "dist/main.js",
  "scripts": {
    "build": "npm run build:main && npm run build:preload && npm run build:renderer",
    "build:main": "webpack --config webpack.main.config.js --mode production",
    "build:preload": "webpack --config webpack.preload.config.js --mode production",
    "build:renderer": "webpack --mode production",
    "dev": "concurrently \"npm run dev:renderer\" \"npm run dev:server\"",
    "dev:renderer": "webpack serve --config webpack.dev.config.js",
    "dev:server": "nodemon server/index.js",
    "dev:main": "webpack --config webpack.main.config.js --mode development --watch",
    "dev:preload": "webpack --config webpack.preload.config.js --mode development --watch",
    "start": "electron .",
    "start:dev": "concurrently \"npm run dev:renderer\" \"npm run dev:main\" \"npm run dev:preload\" \"npm run dev:server\" \"wait-on dist/renderer.js dist/main.js dist/preload.js http://localhost:3001 && electron .\"",
    "test": "echo \"Error: no test specified\" && exit 1",
    "setup-db": "node server/database/setup.js"
  },
  "keywords": [
    "pos",
    "electron",
    "react",
    "typescript"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "date-fns": "^2.30.0",
    "dompurify": "^3.2.4",
    "electron-store": "^8.1.0",
    "jspdf": "^3.0.4",
    "qrcode": "^1.5.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.12.0",
    "react-router-dom": "^6.20.0",
    "axios": "^1.6.2",
    "express": "^4.18.2",
    "sqlite3": "^5.1.6",
    "cors": "^2.8.5",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.23.5",
    "@babel/preset-env": "^7.23.5",
    "@babel/preset-react": "^7.23.2",
    "@babel/preset-typescript": "^7.23.3",
    "@types/dompurify": "^3.0.5",
    "@types/node": "^20.10.4",
    "@types/qrcode": "^1.5.5",
    "@types/react": "^18.2.43",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "babel-loader": "^9.1.3",
    "concurrently": "^8.2.2",
    "css-loader": "^6.8.1",
    "electron": "^39.2.7",
    "eslint": "^8.56.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "html-webpack-plugin": "^5.6.0",
    "style-loader": "^3.3.3",
    "ts-loader": "^9.5.1",
    "typescript": "^5.3.3",
    "wait-on": "^7.2.0",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.2.2",
    "nodemon": "^3.0.2",
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/multer": "^1.4.11",
    "@types/uuid": "^9.0.7"
  }
}
